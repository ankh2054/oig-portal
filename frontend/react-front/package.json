{
  "name": "react-front-v2",
  "version": "0.1.0",
  "private": true,
  "msw": {
    "workerDirectory": "public"
  },
  "scripts": {
    "start": "vite --port 3001",
    "dev": "vite --port 3001",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "serve": "serve dist",
    "test": "vitest --run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts,.tsx,.js,jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,jsx --fix",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --write \"**/*.+(json|yml|css|md|mdx|tsx|ts|js|)\"",
    "clean": "rimraf node_modules package-lock.json dist",
    "validate": "./scripts/validate",
    "builddocker": "tsc && vite build && rm dist/assets/*.map && mkdir -p ../fastify/public && cp -r -f dist/* ../fastify/public/",
    "buildev": "tsc && vite build && rm dist/assets/*.map && mkdir -p ../fastify/public && cp -r -f dist/* ../fastify/public/"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "classnames": "^2.3.2",
    "country-flag-icons": "^1.5.7",
    "dayjs": "^1.11.7",
    "ky": "^0.33.3",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-datepicker": "^4.17.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "react-tooltip": "^5.11.1",
    "recharts": "^2.5.0",
    "regenerator-runtime": "^0.13.11",
    "sort-by": "^0.0.2",
    "ual-anchor": "^1.3.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "@types/react-lazy-load-image-component": "^1.5.2",
    "@types/react-outside-click-handler": "^1.3.1",
    "@types/react-datepicker": "^4.15.0",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-import-resolver-typescript": "^3.5.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "jsdom": "^21.1.1",
    "msw": "^1.2.1",
    "node-fetch": "^3.3.1",
    "postcss": "^8.4.31",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.6",
    "serve": "^14.2.0",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.3",
    "ual-reactjs-renderer": "^0.3.1",
    "vite": "^4.2.3",
    "vitest": "^0.29.8"
  }
}
